@{
    ViewData["Title"] = "Owners";
}
@model List<Owners>

<div class="text-center">
    <h1 class="display-4">Owners</h1>
    <button class="btn btn-primary">Add Owner</button>
    <button class="btn btn-outline-primary">Update Owner</button>
</div>
<div class="d-flex justify-content-around">
@foreach (var owner in Model)
{
    <div class="m-3">
            <div><b>Name: </b>@owner.FirstName @owner.LastName</div>
            <div><b>Driver License: </b>@owner.DriverLicense</div>
    </div>
}
</div>